<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h3>Song Name</h3>
<%= @song.name %>

<h3>Artist</h3>
<a href="/artists/<%= @artist.slug %>"><%= @artist.name %></a>

<h3>Genres</h3>
<% @song.genres.each do |genre| %>
  <a href="/genres/<%= genre.slug %>"><%= genre.name %></a>
<% end %>